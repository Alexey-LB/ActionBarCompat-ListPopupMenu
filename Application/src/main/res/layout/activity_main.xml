<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LayoutMain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="10"
    android:background="@color/colorMinThermometer">
    <!---->
    <!--ЗАСТАВКА РЕЛСИБА -->
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        android:background="@drawable/house_start">

        <FrameLayout
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:id="@+id/frame2">

        </FrameLayout>

        <FrameLayout
            android:id="@+id/frameHeadband"
            android:orientation="horizontal"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:theme="@android:style/TextAppearance.Inverse"
            android:elevation="1dp"


            android:layout_gravity="fill_vertical|fill_horizontal">
            <!--android:background="@drawable/rays"-->
            <!--android:background="@drawable/home"-->
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="bottom|center_vertical"
                android:layout_gravity="bottom|center_vertical">

            </LinearLayout>
            <!--</LinearLayout>-->


        </FrameLayout>
    </LinearLayout>

    <FrameLayout
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/frame_button"
        android:layout_gravity="center_vertical|center_horizontal"
        android:background="@drawable/house_button_transition">

    </FrameLayout>

    <FrameLayout
        android:background="@drawable/house_relsib_logo"
        android:layout_gravity="bottom|right"
        android:layout_marginBottom="35dp"
        android:layout_marginRight="35dp"
        android:layout_width="70dp"
        android:layout_height="70dp">

    </FrameLayout>

    <!--app:srcCompat="@drawable/home"-->
</FrameLayout>
